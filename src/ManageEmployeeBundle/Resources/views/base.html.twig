<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Manage Employee{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" >
        {% block stylesheets %}
            <link href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
            <link href="{{ asset('assets/vendor/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" media="screen">
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/manageemployee/css/base_layout.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="spinner">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </div>
        <div class="background-image" style="display: none;"></div>
        <div class="container-fluid wrapper" style="display: none;">
            {% block header %}{% endblock %}

            {% block body %}{% endblock %}

            {% block footer %}{% endblock %}
        <div/>
        {% block javascripts %}
            <!-- jQuery (necessary for Bootstraps JavaScript plugins) -->
            <script src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
            <!-- Include all JavaScripts, compiled by Assetic -->
            <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
